{
  "name": "ou-card",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "push": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "core-js": "^3.19.3",
    "element-ui": "^2.15.6",
    "nuxt": "^2.15.8",
    "push-dir": "^0.4.1",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "sass": "^1.52.2",
    "sass-loader": "10.1.1"
  }
}
